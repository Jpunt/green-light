{
  "name": "green-light",
  "version": "0.4.7",
  "author": "Jasper Haggenburg",
  "description": "An opinionated (but complete) set of tools for functional testing",
  "license": "ISC",
  "homepage": "https://github.com/jpunt/green-light#readme",
  "keywords": [
    "testing",
    "functional",
    "tests",
    "mocked-api",
    "mocha",
    "chai",
    "jsdom"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpunt/green-light.git"
  },
  "bugs": {
    "url": "https://github.com/jpunt/green-light/issues"
  },
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel src --out-dir lib",
    "prepublish": "rm -rf ./lib && npm run compile",
    "test": "NODE_ENV=green-light mocha --compilers js:babel-register --timeout=5000",
    "lint": "eslint . --max-warnings 0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^7.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.5.1",
    "eslint": "^3.7.1",
    "express": "^4.13.4",
    "mock-require": "^1.2.1"
  },
  "dependencies": {
    "chai": "^3.5.0",
    "command-line-args": "^2.1.4",
    "glob": "^7.0.3",
    "jsdom": "^9.0.0",
    "lodash": "^4.3.0",
    "mocha": "^3.1.0",
    "mocked-api": "^0.6.5",
    "node-fetch": "^1.4.1",
    "promise-poller": "^1.3.0",
    "terminate": "^1.0.8"
  }
}
